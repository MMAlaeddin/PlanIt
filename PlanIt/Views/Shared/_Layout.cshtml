    <div class="dropdown-menu">
      <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Go Home</a>
      <a class="dropdown-item" asp-area="" asp-controller="BusProposal" asp-action="Index">My Proposals</a>
      <a class="dropdown-item" asp-area="" asp-controller="Account" asp-action="Login">Sign In</a>
      <a class="dropdown-item" asp-area="" asp-controller="Account" asp-action="Register">Register</a>
      <a class="dropdown-item" asp-area="" asp-controller="Account" asp-action="Index">Delete My Account</a>
    </div>
    <li class="nav-item dropdown">
    <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Documents</a>
    <div class="dropdown-menu">
      <a class="dropdown-item" href="https://sos.oregon.gov/business/Pages/domestic-business-corporation-forms.aspx">State Registration</a>
